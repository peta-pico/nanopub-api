#+ summary: Find all nanopublications of the given type, in undefined order
#+ method: GET
#+ pagination: 1000

prefix np: <http://www.nanopub.org/nschema#>

select distinct ?np where {
  graph ?h {
    ?np a np:Nanopublication .
    ?np np:hasPublicationInfo ?i .
  }
  graph ?i {
    ?np a ?_type_iri .
  }
}
