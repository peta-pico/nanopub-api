#+ summary: Get nanopublication by identifier (URI)
#+ method: GET

prefix np: <http://www.nanopub.org/nschema#>

select ?g ?s ?p ?o where {
  {
    graph ?g {
      ?_np_iri a np:Nanopublication
    }
  } union
  {
    graph ?h {
      ?_np_iri a np:Nanopublication {
        ?_np_iri np:hasAssertion ?g
      } union {
        ?_np_iri np:hasProvenance ?g
      } union {
        ?_np_iri np:hasPublicationInfo ?g
      }
    }
  }
  graph ?g {?s ?p ?o}
}
